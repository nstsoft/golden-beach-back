{
  "name": "back",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "node ./esbuild.config.mjs",
    "dev": "nodemon --config ./nodemon.json",
    "start": "node ./build/index.cjs",
    "migration:create": "node --loader ts-node/esm ./node_modules/.bin/typeorm migration:create",
    "migration:generate": "node --loader ts-node/esm ./node_modules/.bin/typeorm migration:generate -d ./src/data-source.ts",
    "migration:up": "node --loader ts-node/esm ./node_modules/.bin/typeorm migration:run -d ./src/data-source.ts",
    "migration:down": "node --loader ts-node/esm ./node_modules/.bin/typeorm migration:revert -d ./src/data-source.ts"
  },
  "devDependencies": {
    "@types/aws-serverless-express": "^3.3.9",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^2.0.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.11",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vercel/ncc": "^0.38.1",
    "esbuild": "^0.21.3",
    "eslint": "^9.2.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsc-bundle": "^0.1.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.583.0",
    "@aws-sdk/lib-storage": "^3.583.0",
    "@codegenie/serverless-express": "^4.14.1",
    "@mapbox/node-pre-gyp": "^1.0.11",
    "@tsconfig/node21": "^21.0.3",
    "@types/mongodb": "^4.0.7",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.6.1",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.4",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts"
  },
  "packageManager": "yarn@4.2.2"
}
